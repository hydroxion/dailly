{
  "expo": {
    "name": "dailly",
    "slug": "dailly",
    "description": "Create, manage, track and share your daily goals in a simple and aesthetic way.",
    "entryPoint": "./application.js",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["android"],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 1000
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "resize",
      "allowBackup": false,
      "package": "com.dailly.dailly",
      "versionCode": 4,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.dailly.dailly"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "turboModules": true
    }
  }
}
