{
  "expo": {
    "name": "dailly",
    "slug": "dailly",
    "description": "Create, manage and track your daily goals.",
    "version": "12.0.0",
    "orientation": "portrait",
    "icon": "./assets/application/icon.png",
    "splash": {
      "image": "./assets/application/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["android"],
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/application/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "resize",
      "allowBackup": false,
      "package": "com.dailly.dailly",
      "versionCode": 12,
      "permissions": ["VIBRATE"]
    },
    "web": {
      "favicon": "./assets/application/favicon.png"
    },
    "experiments": {
      "turboModules": true
    },
    "ios": {
      "bundleIdentifier": "com.dailly.dailly"
    }
  }
}
